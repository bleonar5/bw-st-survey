<div id="content-heading-a" class="text-center">
    <div class="row h-100">
        <div class="col-md-12 align-self-end">
            <h2>Informed Consent Form</h2>
        </div>
    </div>
</div>

<div id="content-main-a" class="mx-sm-0 mx-md-1 mx-lg-3">
    <div class="row bespoke-vertical-align-a">
        <div class="col-12 text-left px-sm-0 py-md-1">
            {{#each stdConsent}}
                <p><span class="consent-heading">{{this.subheading}}</span><span>: {{{this.text}}}</span></p>
            {{/each}}
        </div>
    </div>
{{!-- closing div of end of contentmain below --}}
</div>

<div id="footer-a" class="row mx-xs-0 mx-sm-2 mx-md-3 mx-lg-5">
    {{> 4footerleft}}
            <a href="/no-study-consent" role="button" class="btn btn-lg btn-warning float-left">I Do <strong>NOT </strong>Consent</a>
        </div>
    </div>

    {{> 5footermiddle}}
        </div>
    </div>

    {{> 6footerright}}
            <a href="/eligibility-notice" role="button" class="btn btn-lg btn-success float-right">I Consent</a>
        </div>
    </div>
{{!-- closing div of footer below --}}
</div>
