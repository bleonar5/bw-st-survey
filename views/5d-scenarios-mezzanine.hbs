<form method="POST" class="h-100">
<div id="content-heading-g" class="text-center">
    <div class="row h-100">
        <div class="col-md-12 align-self-end">
        </div>
    </div>
</div>

<div id="content-main-g">
    <div class="row justify-content-center bespoke-vertical-align-g">
        <div class="col-xs-12 col-sm-10 col-md-8 text-center">

          {{#each instructions}}
            {{#if info}}
                <h3 class="my-sm-3 my-md-4 my-lg-4 my-xl-5">{{{this.text}}}</h3>
            {{/if}}
          {{/each}}

        </div>
    </div>
{{!-- closing div of end of contentmain below --}}
</div>

<div id="footer-g" class="row">

    <div class="col-2 offset-2">
        <div class="float-left">
            {{#if urlsAndPages.backbutton}}
                <a href={{urlsAndPages.previousPage}} role="button" class="btn btn-lg btn-secondary float-left">Back</a>
            {{/if}}
        </div>
    </div>

    <div class="col-4">
        <div class="text-center">
            
        </div>
    </div>

    <div class="col-2">
        <div class="float-right">
            <button role="button" type="submit" class="btn btn-lg btn-primary hand-curser">Next</button>
        </div>
    </div>
{{!-- closing div of footer below --}}
</div>
</form>